#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/Accesstradepay-/node_modules/jiti/lib/node_modules:/data/data/com.termux/files/home/Accesstradepay-/node_modules/jiti/node_modules:/data/data/com.termux/files/home/Accesstradepay-/node_modules:/data/data/com.termux/files/home/node_modules:/data/data/com.termux/files/node_modules:/data/data/com.termux/node_modules:/data/data/node_modules:/data/node_modules:/node_modules:/data/data/com.termux/files/home/Accesstradepay-/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/Accesstradepay-/node_modules/jiti/lib/node_modules:/data/data/com.termux/files/home/Accesstradepay-/node_modules/jiti/node_modules:/data/data/com.termux/files/home/Accesstradepay-/node_modules:/data/data/com.termux/files/home/node_modules:/data/data/com.termux/files/node_modules:/data/data/com.termux/node_modules:/data/data/node_modules:/data/node_modules:/node_modules:/data/data/com.termux/files/home/Accesstradepay-/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jiti/lib/jiti-cli.mjs" "$@"
else
  exec node  "$basedir/../jiti/lib/jiti-cli.mjs" "$@"
fi
